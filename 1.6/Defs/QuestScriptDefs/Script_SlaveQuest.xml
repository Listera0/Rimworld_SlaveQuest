    <autoAccept>false</autoAccept>
    <isRootSpecial>false</isRootSpecial>
    <expireDaysRange>12~15</expireDaysRange>
    <questNameRules>
      <rulesStrings>
        <li>questName->[slave_category_def] Slave Proposal</li>
